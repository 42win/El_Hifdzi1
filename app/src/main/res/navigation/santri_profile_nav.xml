            android:id="@+id/action_santriProfileFragment_to_santriProfileDetailFragment"
            app:destination="@id/santriProfileDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/santriProfileDetailFragment"
        android:name="com.az.elhifdzi.ui.santri.SantriProfileDetailFragment"
