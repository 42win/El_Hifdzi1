    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/santri_profile_nav"
    app:startDestination="@id/santriProfileFragment">

    <fragment
        android:id="@+id/santriProfileFragment"
        android:name="com.az.elhifdzi.ui.santri.SantriProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_santri_profile" >
